{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/shop", "destination": "/shop.html" },
    { "source": "/about-us", "destination": "/about-us.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/error404", "destination": "/error404.html" },
    { "source": "/faq", "destination": "/faq.html" },
    { "source": "/product-details", "destination": "/product-details.html" },
    { "source": "/terms", "destination": "/terms.html" },
    { "source": "/privacy-policy", "destination": "/privacy-policy.html" },
    { "source": "/disclaimer", "destination": "/disclaimer.html" },
    { "source": "/(.*)", "destination": "/error404.html" }
  ]
}
